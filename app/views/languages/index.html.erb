  <div id="happy_coding_button"></div>
  <div id="code_example">
    <pre class="code_example_view">
    </pre>
    <pre class="code_example_view2"></pre>
  </div>
  <div class="styled-select">

    <div id="error_messages">
      
    </div>
      <h1>I know <%= collection_select(:language, :language_id, @languages, :id, :name, {:prompt => 'SELECT LANGUAGE'}, {:id => 'start_language'}) %></h1><br>

